<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  
  <display-name>VideoFinder</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>memberEnroll</servlet-name>
    <jsp-file>/WEB-INF/views/member/member.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>memberEnroll</servlet-name>
    <url-pattern>/member/memberEnroll</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>memberLogin</servlet-name>
    <jsp-file>/WEB-INF/views/member/memberLogin.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>memberLogin</servlet-name>
    <url-pattern>/member/memberLogin</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>findInfo</servlet-name>
    <jsp-file>/WEB-INF/views/member/findInfoId.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>findInfo</servlet-name>
    <url-pattern>/member/findInfoId</url-pattern>
  </servlet-mapping>
</web-app>